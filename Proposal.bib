@Article{Mooney,
  Title                    = {{Continuously-variable series-elastic actuator.}},
  Author                   = {Mooney, Luke and Herr, Hugh},
  Journal                  = {IEEE ... International Conference on Rehabilitation Robotics : [proceedings]},
