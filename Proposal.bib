  ISSN                     = {22109838},
  Keywords                 = {Biological joints,Gait simulation,Musculotendinous actuators,Neuromuscular control,Neuromusculoskeletal biomechanics,Simbios biocomputation,Simtk simbody},
  Owner                    = {nikorose},
