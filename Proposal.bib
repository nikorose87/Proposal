  ISSN                     = {09666362},
  Keywords                 = {Biomechanics,Locomotion,Low back pain,Musculoskeletal modeling,Static optimization},
  Mendeley-tags            = {Biomechanics,Locomotion,Low back pain,Musculoskeletal modeling,Static optimization},
  Owner                    = {Ing. Edwin N. Prieto},
  Timestamp                = {2015.04.03},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0966636215000193}
}

