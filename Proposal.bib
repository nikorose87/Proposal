  Keywords                 = {Adult,Amputation,Amputees,Below-knee,Biomechanical Phenomena,Biomechanics,Electromyography,Forward dynamics,Gait,Gait: physiology,Humans,Joint loading,Knee Joint,Knee Joint: physiology,Leg,Leg: physiology,Locomotion,Mechanical Processes,Middle Aged,Muscle, Skeletal,Muscle, Skeletal: physiology,Osteoarthritis, Knee,Osteoarthritis, Knee: complications,Osteoarthritis, Knee: physiopathology,Tibia,Tibia: surgery,Walking,Walking: physiology,Young Adult},
  Mendeley-tags            = {Below-knee,Biomechanics,Forward dynamics,Joint loading,Locomotion},
  Owner                    = {Ing. Edwin N. Prieto},
  Pmid                     = {24972921},
  Timestamp                = {2015.04.03},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0021929014003455}
}

