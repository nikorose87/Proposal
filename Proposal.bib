  ISBN                     = {2210-9838},
  ISSN                     = {22109838},
  Keywords                 = {Biological joints,Biomedical simulation,Biomolecular simulation,Compliant contact,Coupled motion,Minimal coordinates,Neuromuscular simulation,Open source,Real time simulation},
  Mendeley-groups          = {Prosthetics/Rigid Body Dynamics},
  Owner                    = {nikorose},
