  Keywords                 = {Activities of Daily Living,Adult,Aged,Amputees,Amputees: rehabilitation,Artificial Limbs,Cognition,Cognition: physiology,Female,Humans,Knee Prosthesis,Male,Microcomputers,Middle Aged,Patient Satisfaction,Prosthesis Design,Questionnaires,Walking,Walking: physiology},
  Owner                    = {nikor_000},
